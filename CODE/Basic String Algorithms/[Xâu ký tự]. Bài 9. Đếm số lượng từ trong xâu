const int INF = 1e9 + 1;
const ll INFF = 1e18 + 1;
const int MaxN = 1e6 + 1;
const int MOD = 1e9 + 7;

namespace solve
{
	void process(void)
	{
		string str;
		getline(cin, str);
		str.push_back(' ');
		int cnt = 0;
		FL(i, 1, sz(str))
			if(str[i] == ' ' && str[i - 1] != ' ')
				cnt++;
		cout << cnt;
	}
}


