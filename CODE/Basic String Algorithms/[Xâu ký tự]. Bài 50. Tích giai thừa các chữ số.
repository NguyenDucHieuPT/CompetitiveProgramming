const int INF = 1e9 + 1;
const ll INFF = 1e18 + 1;
const int MaxN = 1e6 + 1;
const ll MOD = 1e9 + 7;

namespace solve
{
	vector<int> prime_factor(int n)
	{
		int i = 2;
		vector<int> res;
		while(i <= sqrt(n))
		{
			while(n % i == 0)
			{
				res.push_back(i)
			}
			i++;
		}
	}
	void process(void)
	{
		ll a, x, y;
    	cin >> a >> x >> y;
	    ll gcd_xy = __gcd(x, y);
	    for(ll i = 0; i < gcd_xy; i++)
	    	cout << a;
	}	
}


HaCam
{
    fastio;
    file("B");
    solve::process();

    
    return 0;
}